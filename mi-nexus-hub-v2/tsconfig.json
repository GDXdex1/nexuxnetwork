{
  "compilerOptions": {
    "jsx": "react-jsx",        // ðŸ‘ˆ IMPORTANTE
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"]
}